@model IEnumerable<WebApplication29.Models.CvJob>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.YearsOfExperiance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastWorkPlace)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JobDescription)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Category)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.User)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.YearsOfExperiance)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastWorkPlace)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JobDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Category.CategoryId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.User.UserId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CvId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CvId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CvId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
